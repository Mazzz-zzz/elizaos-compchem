{
  "name": "@elizaos-plugins/plugin-gaussian-kg",
  "version": "0.1.0",
  "description": "Eliza OS plugin for parsing Gaussian 16 quantum chemistry logfiles into a knowledge graph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:python": "cd py && poetry run pytest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "keywords": [
    "eliza",
    "elizaos",
    "plugin",
    "quantum-chemistry",
    "gaussian",
    "knowledge-graph",
    "rdf",
    "bioai",
    "chemistry"
  ],
  "author": "Bio x AI Hackathon Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.15.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elizaos/plugin-bootstrap": "1.0.2",
    "@elizaos/plugin-knowledge": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "py",
    "characters",
    "README.md"
  ],
  "engines": {
    "node": ">=23.0.0"
  }
}